Host bitbucket.org
    HostName        bitbucket.org
    User            yourID
    Port 22
    IdentityFile    ~/.ssh/your_rsa
    TCPKeepAlive yes
    IdentitiesOnly yes
Host github.com
    HostName        github.com
#User is not you id
    User            git
    Port 22
    IdentityFile    ~/.ssh/your_rsa
    TCPKeepAlive yes
    IdentitiesOnly yes
